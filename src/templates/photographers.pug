doctype html
html
    head
        title Фотографы
        meta(name="description", content="")
        meta(name="keywords", content="")
        meta(charset="UTF-8") 
        meta(name='viewport' content='width=device-width,initial-scale=1')
        meta(content='true' name='HandheldFriendly')
        meta(content='width' name='MobileOptimized')
        meta(content='yes' name='apple-mobile-web-app-capable')
        link(rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap")
        link(rel="stylesheet" type="text/css" href="assets/css/all.min.css")

    body
        div.problems-money
            div.container.c-container
                div.row
                    div.col-md-12
                        .header-row-small
                            a.back(href="index.html")
                                img(src="assets/img/arrow-back-small.svg" alt="назад")
                                span Назад
                            h3.h3-small Фотографы в Крыму

                        a.img-block(href="#")
                            .header(style="background-image: url('assets/img/photographers-1.png');")
                                span.span1 Макс Фрайер
                                span.span2 Из любой девушки могу сделать фотомодель
                            .detailed Подробнее

                        a.img-block(href="#")
                            .header(style="background-image: url('assets/img/photographers-2.png');")
                                span.span1 Оля Филипс
                                span.span2 Все будет красиво!
                            .detailed Подробнее

                        .swiper-container.categories
                            .header Остальные категории
                            .swiper-wrapper
                                .swiper-slide
                                    a.link(href="#")
                                        img(src="assets/img/cat-photo.svg" alt="фотосессии")
                                        span.text Фотосессии
                                .swiper-slide
                                    a.link(href="#")
                                        img(src="assets/img/cat-exc.svg" alt="индивидуальные экскурсии")
                                        span.text Индивидуальные экскурсии
                                .swiper-slide
                                    a.link(href="#")
                                        img(src="assets/img/cat-relax.svg" alt="отдых с детьми")
                                        span.text Отдых с детьми
                                .swiper-slide
                                    a.link(href="#")
                                        img(src="assets/img/cat-eno.svg" alt="энотуризм")
                                        span.text Энотуризм
                                .swiper-slide
                                    a.link(href="#")
                                        img(src="assets/img/cat-photo.svg" alt="фотосессии")
                                        span.text Фотосессии
                                .swiper-slide
                                    a.link(href="#")
                                        img(src="assets/img/cat-exc.svg" alt="индивидуальные экскурсии")
                                        span.text Индивидуальные экскурсии

       
        script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js")
        script(src="assets/js/app.min.js")